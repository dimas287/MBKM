            },
            body: 'action=check_auth'
        })
        .then(response => response.json())
        .then(data => {
            if (data.status === 'success') {
                updateAdminInfo(data.data);
            } else {
                // Redirect ke halaman user jika belum login
                window.location.href = 'user.html';
